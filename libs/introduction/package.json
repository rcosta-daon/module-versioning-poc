{
  "name": "@module-versioning-poc/introduction",
  "version": "0.5.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "predeploy": "nx build introduction",
    "deploy": "gh-pages --dist dist --add --dest introduction/$npm_package_version/"
  }
}
